<UserControl x:Class="Screenshot.Resources.UC_SelectTool"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Screenshot.Resources"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="Wheat">
        <Border  Name="SelectBoder" BorderThickness="1" Background="Blue" Height="100"  Width="100"></Border>
        <Ellipse Name="Ell_LeftTop"></Ellipse>
        <Ellipse Name="Ell_LeftDown"></Ellipse>
        <Ellipse Name="Ell_LeftMid"></Ellipse>
        <Ellipse Name="Ell_TopMid"></Ellipse>
        <Ellipse Name="Ell_RightTop"></Ellipse>
        <Ellipse Name="Ell_RightMid"></Ellipse>
        <Ellipse Name="Ell_RightDown"></Ellipse>
        <Ellipse Name="Ell_DownMid"></Ellipse>
    </Grid>
</UserControl>